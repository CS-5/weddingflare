{
  "name": "weddingflare",
  "version": "1.0.0",
  "private": true,
  "author": "CS-5",
  "license": "MIT",
  "scripts": {
    "dev": "npm run wrangler:dev",
    "build": "npm run next:build && npm run next:export",
    "next:dev": "next dev",
    "next:build": "next build",
    "next:export": "next export",
    "next:start": "next start",
    "next:lint": "next lint",
    "wrangler:dev": "dotenv -c -- bash -c 'wrangler pages dev --kv WF_RSVP_RESPONSES --binding WF_SHEETS_ENDPOINT=\"$WF_SHEETS_ENDPOINT\" -- npm run next:dev'"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.4.0",
    "@tailwindcss/forms": "^0.5.0",
    "@tailwindcss/typography": "^0.5.2",
    "@types/node": "^17.0.23",
    "@types/react": "^17.0.43",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "autoprefixer": "^10.4.4",
    "dotenv-cli": "^5.1.0",
    "eslint": "8.12.0",
    "eslint-config-next": "12.1.3",
    "eslint-config-prettier": "^8.5.0",
    "postcss": "^8.4.12",
    "prettier": "^2.6.1",
    "tailwindcss": "^3.0.23",
    "tailwindcss-textshadow": "^2.1.3",
    "typescript": "4.6.3",
    "wrangler": "^0.0.24"
  },
  "dependencies": {
    "@builder.io/partytown": "^0.5.1",
    "@react-google-maps/api": "^2.7.0",
    "next": "^12.1.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.29.0",
    "uuid": "^8.3.2"
  },
  "eslintConfig": {
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "next/core-web-vitals",
      "prettier"
    ],
    "rules": {
      "@next/next/no-img-element": "off"
    }
  }
}
